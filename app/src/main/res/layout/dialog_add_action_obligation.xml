<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/g_LayoutButton1"    
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include android:id="@+id/action_bar"
        layout="@layout/view_action_bar"></include>

    <RelativeLayout
        android:layout_margin="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:id="@+id/layout1"
                android:weightSum="10">

                <LinearLayout
                    android:layout_weight="6.8"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:id="@+id/customers"
                        android:paddingRight="2dp"
                        android:maxLines="2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        />



                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="3.2">
                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/customer_txt"
                        android:text="@string/title_activity_customers"/>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:id="@+id/layout1"
                android:weightSum="10">

                <LinearLayout
                android:layout_weight="6.8"
                android:layout_width="0dp"
                android:paddingRight="2dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:id="@+id/project"
                        android:gravity="right"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:gravity="right"
                        android:id="@+id/date"
                        android:paddingLeft="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="5dp"
                        android:weightSum="3">

                        <TextView
                        android:paddingLeft="5dp"
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:id="@+id/until"
                        android:gravity="right"
                        android:layout_weight="1.2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />

                        <TextView
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:layout_width="0dp"
                            android:layout_weight="0.5"
                            android:paddingRight="10dp"
                            android:layout_height="wrap_content"
                            android:text="@string/until2"
                            android:id="@+id/until_txt" />

                        <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:id="@+id/hour"
                        android:gravity="right"
                        android:layout_weight="1.3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />

                    </LinearLayout>
                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:id="@+id/hour_s"
                        android:paddingTop="7dp"
                        android:gravity="right"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:id="@+id/type"
                        android:gravity="right"
                        android:paddingTop="7dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:layout_gravity="right"
                        android:id="@+id/status"
                        android:paddingTop="7dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />



            </LinearLayout>
                <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="3.2">

                <TextView
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/projectTitle"
                    android:id="@+id/project_txt" />

                <TextView
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/date"
                    android:id="@+id/date_txt" />

                <TextView
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:id="@+id/hour_txt"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/hour_title"/>

                <TextView
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:layout_width="match_parent"
                    android:id="@+id/during"
                    android:layout_height="wrap_content"
                    android:text="@string/hour_s"/>

                <TextView
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:id="@+id/type_txt"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/title_activity_type"/>

                <TextView
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/status_txt"
                    android:paddingTop="7dp"
                    android:text="@string/title_activity_status"/>
            </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:id="@+id/locationView"
                android:weightSum="10">

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:gravity="right"
                        android:paddingRight="5dp"
                        android:layout_weight="6.8"
                        android:layout_width="0dp"
                        android:id="@+id/location"
                        android:maxLines="3"
                        android:layout_height="wrap_content"></TextView>


                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:layout_width="0dp"
                        android:paddingTop="7dp"
                        android:layout_weight="3.2"
                        android:gravity="right"
                        android:layout_height="match_parent"
                        android:text="@string/location"
                        android:id="@+id/location_txt" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:id="@+id/presentView"
                android:weightSum="10">

                <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:gravity="right"
                        android:paddingRight="5dp"
                        android:id="@+id/present"
                        android:maxLines="3"
                        android:layout_weight="6.8"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        />

                <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:layout_weight="3.2"
                        android:id="@+id/present_textView2"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/present"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="match_parent"
                android:id="@+id/descriptionView"
                android:weightSum="10">

                <LinearLayout
                    android:layout_weight="6.8"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:gravity="right"
                        android:paddingRight="5dp"
                        android:id="@+id/description"
                        android:maxLines="3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="3.2">

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:id="@+id/description_textView3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/description"/>

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="match_parent"
                android:id="@+id/layout2"
                android:weightSum="10">

                <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:id="@+id/sum"
                        android:maxLines="3"
                        android:paddingTop="7dp"
                        android:gravity="right"
                        android:paddingRight="5dp"
                        android:layout_weight="6.8"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        />

                <TextView
                        android:layout_weight="3.2"
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:id="@+id/sum_textView4"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/sum"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:id="@+id/layout1"
                android:weightSum="10">

                <LinearLayout
                    android:layout_weight="6.8"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:paddingTop="7dp"
                        android:weightSum="3"
                        android:id="@+id/treatment"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:paddingLeft="5dp"
                            android:id="@+id/treatment_hour"
                            android:gravity="right"
                            android:layout_weight="0.7"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            />

                        <TextView
                            android:gravity="right"
                            android:textColor="#555"
                            android:id="@+id/treatment_hour_txt"
                            android:textSize="17dp"
                            android:layout_width="0dp"
                            android:paddingRight="10dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:text="@string/hour_title"/>

                        <TextView
                            android:gravity="right"
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:paddingRight="5dp"
                            android:id="@+id/treatment_date"
                            android:layout_weight="1.3"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_marginTop="7dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/close"
                        android:weightSum="3">

                        <TextView
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:id="@+id/presentClose"
                            android:layout_weight="0.7"
                            android:layout_width="0dp"
                            android:gravity="right"
                            android:layout_height="wrap_content"
                            />

                        <TextView
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:paddingRight="10dp"
                            android:layout_width="0dp"
                            android:gravity="right"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:id="@+id/presentCloseTxt"
                            android:text="@string/presentClose"/>

                        <TextView
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:paddingLeft="5dp"
                            android:paddingRight="5dp"
                            android:layout_weight="1.3"
                            android:gravity="right"
                            android:id="@+id/sumClose"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            />

                    </LinearLayout>

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:gravity="right"
                        android:paddingTop="7dp"
                        android:id="@+id/chance"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:gravity="right"
                        android:id="@+id/status_chance"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="3.2">

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/treatment_date"
                        android:id="@+id/treatment_date_txt" />

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:id="@+id/sumCloseTxt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/sumClose"/>

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:id="@+id/chanceTxt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/title_activity_chance"/>

                    <TextView
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/status_chanceTxt"
                        android:text="@string/title_activity_status"/>


                </LinearLayout>

            </LinearLayout>
            <LinearLayout
                android:layout_gravity="right"
                android:gravity="right"
                android:id="@+id/email"
                android:weightSum="10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <CheckBox
                    android:id="@+id/send"
                    android:layout_marginTop="-5dp"
                    android:layout_marginRight="-5dp"
                    android:gravity="right"
                    android:scaleX="0.70"
                    android:scaleY="0.70"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:layout_weight="1.2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/send_email"
                    android:id="@+id/textView6" />

            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>




