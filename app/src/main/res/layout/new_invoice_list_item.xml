<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/category_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clickable="false"
    android:focusable="false"
    android:focusableInTouchMode="false"
    android:background="@drawable/grid_item"
    android:layout_margin="10dp"
    android:gravity="center"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="10"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/pay_date"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:layout_weight="2.9"
                android:text=""
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:paddingBottom="5dp"
                android:paddingRight="5dp"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp" />

            <TextView
                android:id="@+id/category_item_mail"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:layout_weight="1.5"
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:paddingBottom="5dp"
                android:singleLine="true"
                android:text="@string/document_value"
                android:paddingRight="5dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp" />



            <TextView
                android:id="@+id/date"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:layout_weight="2.8"
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:paddingBottom="5dp"
                android:paddingRight="5dp"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp" />

            <TextView
                android:id="@+id/category_item_mail"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:layout_weight="2.8"
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:singleLine="true"
                android:text="@string/document_date"
                android:paddingRight="5dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="10"
            android:orientation="horizontal">



            <TextView
                android:id="@+id/doc"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:paddingRight="5dp"
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:layout_weight="7.2"
                android:paddingBottom="5dp"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp" />

            <TextView
                android:id="@+id/category_item_mail"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:layout_weight="2.8"
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:paddingBottom="5dp"
                android:singleLine="true"
                android:text="@string/document"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="10"
            android:orientation="horizontal">




            <TextView
                android:id="@+id/asmachta"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:layout_weight="7.2"
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:paddingBottom="5dp"
                android:singleLine="true"
                android:paddingRight="5dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp" />

            <TextView
                android:id="@+id/category_item_mail"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:layout_weight="2.8"
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:paddingBottom="5dp"
                android:singleLine="true"
                android:text="@string/documentt"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="10"
            android:orientation="horizontal"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="10"
            android:orientation="horizontal">

            <TextView
                android:paddingRight="5dp"
                android:id="@+id/balance"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:layout_weight="2"
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:paddingBottom="5dp"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp"
                />

            <TextView
                android:id="@+id/balanceTxt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:layout_weight="3.2"
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:paddingBottom="5dp"
                android:paddingRight="5dp"
                android:singleLine="true"
                android:text="@string/payment_balance"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp" />

            <TextView
                android:id="@+id/sum"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:layout_weight="2"
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:paddingRight="5dp"
                android:paddingBottom="5dp"
                android:singleLine="true"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp" />

            <TextView
                android:id="@+id/category_item_mail"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:layout_weight="2.8"
                android:focusableInTouchMode="false"
                android:gravity="right"
                android:paddingBottom="5dp"
                android:singleLine="true"
                android:text="@string/summ"
                android:paddingRight="5dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#555"
                android:textSize="17dp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="10"
            android:orientation="horizontal">




        </LinearLayout>


    </LinearLayout>

</LinearLayout>