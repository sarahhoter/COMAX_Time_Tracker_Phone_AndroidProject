<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/g_LayoutButton1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical">
    <include android:id="@+id/action_bar"
        layout="@layout/view_action_bar"></include>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_gravity="center|center_vertical"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        android:orientation="vertical">

        <LinearLayout

            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:id="@+id/layout1"
            android:weightSum="10">

            <LinearLayout
                android:layout_weight="7"
                android:layout_width="0dp"
                android:layout_marginLeft="20dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:gravity="right"
                    android:layout_gravity="right"
                    android:inputType="number"
                    android:layout_width="0dp"
                    android:layout_weight="0.5"
                    android:layout_marginRight="5dp"
                    android:layout_height="wrap_content"
                    />

                <TextView
                    android:id="@+id/store"
                    android:gravity="right"
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:layout_marginRight="5dp"
                    android:layout_gravity="right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="3">


                <TextView

                    android:textColor="#555"
                    android:layout_gravity="right"
                    android:layout_marginRight="20dp"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:paddingRight="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/from_store"/>



            </LinearLayout>

        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:id="@+id/layout1"
            android:weightSum="10">

            <LinearLayout
                android:layout_weight="7"
                android:layout_width="0dp"
                android:layout_marginLeft="20dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:gravity="right"
                    android:layout_gravity="right"
                    android:inputType="number"
                    android:layout_width="0dp"
                    android:layout_weight="0.5"
                    android:layout_marginRight="5dp"
                    android:layout_height="wrap_content"
                    />

                <TextView
                    android:id="@+id/for_store"
                    android:gravity="right"
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:layout_marginRight="5dp"
                    android:layout_gravity="right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="3">


                <TextView
                    android:id="@+id/quantity_review"
                    android:textColor="#555"
                    android:layout_gravity="end"
                    android:layout_marginRight="20dp"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:paddingRight="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/for_store"/>



            </LinearLayout>

        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:id="@+id/layout1"
            android:weightSum="10">

            <LinearLayout
                android:layout_weight="7"
                android:layout_width="0dp"
                android:layout_marginLeft="20dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:gravity="right"
                    android:layout_gravity="right"
                    android:inputType="number"
                    android:layout_width="0dp"
                    android:layout_weight="0.5"
                    android:layout_marginRight="5dp"
                    android:layout_height="wrap_content"
                    />
                <EditText
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:gravity="right"
                    android:background="@drawable/bg_border_bottom_medium_gray"
                    android:layout_gravity="end"
                    android:id="@+id/documenttVal"
                    android:inputType="number"

                    android:layout_width="85dp"
                    android:layout_marginRight="5dp"
                    android:layout_height="wrap_content"><requestFocus /></EditText>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="3">


                <TextView
                    android:id="@+id/quantity_review"
                    android:textColor="#555"
                    android:layout_gravity="end"
                    android:layout_marginRight="20dp"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:paddingRight="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/documentt"/>



            </LinearLayout>

        </LinearLayout>

    </LinearLayout>




</LinearLayout>




