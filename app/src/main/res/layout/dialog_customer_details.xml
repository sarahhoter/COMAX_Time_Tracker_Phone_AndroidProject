<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/g_LayoutButton1"    
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical">
    <include android:id="@+id/action_bar"
        layout="@layout/view_action_bar"></include>
    <include
        android:id="@+id/navigationByCustomerView"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        layout="@layout/view_prev_next_navigation" />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="vertical">

        <LinearLayout
            android:layout_margin="10dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:id="@+id/layout1"
                android:weightSum="10">

                <LinearLayout
                    android:layout_weight="8.5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/telphoneCall"
                            android:textColor="#555"
                            android:layout_marginRight="5dp"
                            android:visibility="gone"
                            android:textSize="17dp"
                            android:layout_gravity="end"
                            android:paddingTop="7dp"
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:text="@string/call"/>

                        <TextView
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:layout_gravity="end"
                            android:paddingLeft="5dp"
                            android:paddingTop="7dp"
                            android:id="@+id/telVal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/pelphoneCall"
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:layout_gravity="end"
                            android:visibility="gone"
                            android:layout_marginRight="5dp"
                            android:paddingTop="7dp"
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:text="@string/call"/>
                        <TextView
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:paddingTop="7dp"
                            android:layout_gravity="end"
                            android:id="@+id/pelVal"
                            android:paddingLeft="5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>






                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1.5">



                        <TextView
                            android:id="@+id/telphone"
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:layout_gravity="end"
                            android:paddingTop="7dp"
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:text="@string/telphone"/>

                    <TextView
                        android:id="@+id/phone"
                        android:layout_gravity="end"
                        android:textColor="#555"
                        android:textSize="17dp"
                        android:paddingTop="7dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/phone"/>


                </LinearLayout>

            </LinearLayout>
        </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content"
                    android:id="@+id/layout1"
                    android:weightSum="10">

                    <LinearLayout
                        android:layout_weight="8.5"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">


                        <TextView
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:layout_gravity="end"
                            android:id="@+id/adressVal"
                            android:layout_weight="1"
                            android:maxLines="2"
                            android:paddingTop="7dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />





                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_weight="1.5">


                        <TextView
                            android:id="@+id/address"
                            android:textColor="#555"
                            android:layout_gravity="end"
                            android:textSize="17dp"
                            android:paddingTop="7dp"
                            android:layout_width="90dp"
                            android:layout_height="wrap_content"
                            android:text="@string/address"/>





                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content"
                    android:id="@+id/layout1"
                    android:weightSum="10">

                    <LinearLayout
                        android:layout_weight="8.5"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">





                        <TextView
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:id="@+id/grpVal"
                            android:layout_gravity="end"
                            android:layout_weight="1"
                            android:paddingTop="7dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />

                        <TextView
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:id="@+id/emailVal"
                            android:layout_weight="1"
                            android:paddingTop="7dp"
                            android:layout_gravity="end"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_weight="1.5">



                        <TextView
                            android:id="@+id/group"
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:layout_gravity="end"
                            android:paddingTop="7dp"
                            android:layout_width="90dp"
                            android:layout_height="wrap_content"
                            android:text="@string/group"/>

                        <TextView
                            android:id="@+id/email"
                            android:textColor="#555"
                            android:textSize="17dp"
                            android:paddingTop="7dp"
                            android:layout_gravity="end"
                            android:layout_width="90dp"
                            android:layout_height="wrap_content"
                            android:text="@string/email"/>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
        <Button
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:text="@string/open_invoice"
            android:textColor="#555"
            android:textSize="17dp"
            android:background="@drawable/white_button"
            android:id="@+id/new_invoice" />

        <LinearLayout
            android:id="@+id/comboGraph"
            android:visibility="visible"
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:weightSum="10"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="6"
                android:layout_height="40dp"
                android:layout_marginRight="10dp"
                android:layout_gravity="center_vertical"
                android:background="@drawable/white_button"
                android:padding="5dp" >

                <Spinner
                android:textColor="#555"
                android:layout_gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:id="@+id/by"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#ffff"
                android:gravity="right"
                android:drawSelectorOnTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentStart="true" />

                <ImageView
                android:id="@+id/image_sort_by"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="5sp"
                android:layout_gravity="center_vertical|right"
                android:layout_marginRight="5dp"
                android:src="@drawable/downarrow" />


            </RelativeLayout>

            <TextView
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:text="@string/by_doc"
                android:gravity="center_vertical"
                android:textColor="#555"
                android:textSize="17dp"
                android:layout_weight="4"
                android:id="@+id/textView4" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="100"
            android:layout_marginTop="5dp"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/gioolHovot"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_gravity="center"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_gravity="right"
                    android:background="#000000"
                    android:layout_height="0.25dp"/>

                    <LinearLayout
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="right"
                    android:weightSum="100" >

                    <LinearLayout
                        android:layout_width="0.25dp"
                        android:layout_gravity="right"
                        android:background="#000000"
                        android:layout_height="match_parent"/>

                    <TextView
                    android:id="@+id/money"
                    android:text="@string/sum_title"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="50"
                        android:paddingRight="5dp"
                    android:gravity="right" />

                    <LinearLayout
                        android:layout_width="0.25dp"
                        android:layout_gravity="right"
                        android:background="#000000"
                        android:layout_height="match_parent"/>

                    <TextView
                    android:id="@+id/month"
                    android:layout_width="0dp"
                    android:text="@string/months"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="50"
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:gravity="center"/>

                    <LinearLayout
                        android:layout_width="0.25dp"
                        android:layout_gravity="right"
                        android:background="#000000"
                        android:layout_height="match_parent"/>

                </LinearLayout>

                    <TableLayout
                    android:id="@+id/table"
                    android:layout_width="200dp"
                    android:layout_gravity="right"
                    android:layout_height="match_parent"
                    android:orientation="vertical" >
                </TableLayout>
                    <LinearLayout
                    android:layout_width="200dp"
                    android:layout_gravity="right"

                    android:background="#000000"
                    android:layout_height="0.25dp"></LinearLayout>




                </LinearLayout>

            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_weight="50"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:gravity="center"
                    android:id="@+id/by_date_title"
                    android:visibility="gone"
                    android:text="@string/by_document_date"/>

                <LinearLayout
                    android:id="@+id/graphDateDoc"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:background="#ffffff"
                    android:orientation="horizontal">

                </LinearLayout>


            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
    </ScrollView>



</LinearLayout>




