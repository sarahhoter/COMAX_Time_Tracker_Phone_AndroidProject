<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/g_LayoutButton1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical">
    <include android:id="@+id/action_bar"
        layout="@layout/view_action_bar"></include>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_gravity="center|center_vertical"
        android:layout_height="match_parent"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.8"
            android:layout_marginRight="5dp"
            android:orientation="vertical"
            android:layout_height="wrap_content">

        <TextView
            android:id="@+id/store"
            android:gravity="right"
            android:textColor="#555"
            android:textSize="17dp"
            android:paddingTop="7dp"
            android:layout_marginRight="5dp"
            android:layout_gravity="right"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/suplier"
                android:gravity="right"
                android:textColor="#555"
                android:textSize="17dp"
                android:paddingTop="7dp"
                android:layout_marginRight="5dp"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:gravity="right"
                    android:layout_gravity="right"
                    android:inputType="number"
                    android:layout_width="0dp"
                    android:layout_weight="0.5"
                    android:layout_marginRight="5dp"
                    android:layout_height="wrap_content"
                    />
                <EditText
                    android:textColor="#555"
                    android:textSize="17dp"
                    android:paddingTop="7dp"
                    android:gravity="right"
                    android:background="@drawable/bg_border_bottom_medium_gray"
                    android:layout_gravity="end"
                    android:id="@+id/documentVal"
                    android:inputType="number"
                    android:layout_width="85dp"
                    android:layout_marginRight="5dp"
                    android:layout_height="wrap_content"><requestFocus /></EditText>

            </LinearLayout>



        </LinearLayout>
        <LinearLayout
            android:layout_marginRight="10dp"
            android:layout_width="0dp"
            android:layout_weight="0.2"
            android:orientation="vertical"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/quantity_review"
                android:textColor="#555"
                android:layout_gravity="right"
                android:gravity="right"
                android:textSize="17dp"
                android:paddingTop="7dp"
                android:paddingRight="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/store"/>
            <TextView
                android:id="@+id/quantity_review"
                android:textColor="#555"
                android:layout_gravity="left"
                android:textSize="17dp"
                android:gravity="right"

                android:paddingTop="7dp"
                android:paddingRight="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/supplier"/>
            <TextView
                android:id="@+id/quantity_review"
                android:textColor="#555"
                android:layout_gravity="left"
                android:textSize="17dp"
                android:gravity="right"

                android:paddingTop="7dp"
                android:paddingRight="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/documentt"/>

        </LinearLayout>

        </LinearLayout>





</LinearLayout>




